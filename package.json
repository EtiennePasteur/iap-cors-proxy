{
  "name": "reverse-proxy-iap",
  "author": "Etienne PASTEUR <me@etiennepasteur.com>",
  "productName": "IAP - CORS - Proxy",
  "version": "1.0.0",
  "description": "IAP - CORS - Proxy",
  "homepage": "./",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@electron/remote": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.0",
    "express": "^4.17.1",
    "google-auth-library": "^7.0.4",
    "http-proxy-middleware": "^1.2.0",
    "http-shutdown": "^1.2.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.4",
    "electron-icon-maker": "^0.0.5",
    "electron-packager": "^15.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./assets/icons/icon",
        "name": "IAP - CORS - Proxy",
        "executableName": "reverse-proxy-iap"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "IAP-CORS-Proxy",
            "authors": "Etienne PASTEUR <me@etiennepasteur.com>"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": "./assets/icons/icon.png"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "icon": "./assets/icons/icon.png"
          }
        }
      ]
    }
  }
}
