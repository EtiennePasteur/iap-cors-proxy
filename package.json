{
  "name": "iap-cors-proxy",
  "author": "Etienne PASTEUR",
  "productName": "iap-cors-proxy",
  "version": "1.0.2",
  "description": "Reverse proxy to handle CORS and IAP Auth for easier frontend development",
  "homepage": "./",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-mac": "electron-forge make --platform darwin",
    "make-linux": "electron-forge make --platform linux",
    "make-windows": "electron-forge make --platform win32"
  },
  "keywords": [
    "IAP",
    "CORS",
    "Proxy",
    "Node.js"
  ],
  "license": "MIT",
  "dependencies": {
    "@electron/remote": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.0",
    "express": "^4.17.1",
    "google-auth-library": "^7.0.4",
    "http-proxy-middleware": "^1.2.0",
    "http-shutdown": "^1.2.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.4",
    "electron-icon-maker": "^0.0.5",
    "electron-packager": "^15.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./assets/icons/icon",
        "name": "iap-cors-proxy",
        "executableName": "iap-cors-proxy"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "iap-cors-proxy"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": "./assets/icons/icon.png"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "icon": "./assets/icons/icon.png"
          }
        }
      ]
    }
  }
}
